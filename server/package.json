{
  "name": "demo",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "seed": "npx tsx ./seed",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "prebuild": "(rimraf dist || rm -rf dist) && (rimraf upload.zip || rm -rf upload.zip)",
    "build": "npm run typescript && node esbuild.config.mjs",
    "typescript": "tsc --noEmit --pretty",
    "typescript:watch": "tsc --noEmit --watch",
    "deploy": "npm run build && npm run zip",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:verbose": "vitest --reporter verbose",
    "test:run": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "zip": "bestzip upload.zip dist/* package.json"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.24.2",
    "express": "^4.21.2",
    "html-entities": "^2.6.0",
    "mongoose": "^8.9.4",
    "source-map-support": "^0.5.21",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.6",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/ui": "^2.1.4",
    "bestzip": "^2.2.1",
    "esbuild-copy-static-files": "^0.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vitest": "^0.5.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
